# Configuration Frontend Cardify - Production

# URL de l'API backend - À MODIFIER après déploiement Render
VITE_API_URL=https://cardify-backend-XXXX.onrender.com

# URL de base pour l'API
VITE_API_BASE_URL=https://cardify-backend-XXXX.onrender.com/api

# Nom de l'application
VITE_APP_NAME=Cardify

# Version de l'application
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=production

# Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif,image/webp
